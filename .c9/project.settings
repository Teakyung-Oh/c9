{
    "build": {
        "@saveall": true
    },
    "format": {
        "jsbeautify": {
            "@formatOnSave": false,
            "@indent_inner_html": false,
            "@keeparrayindentation": false
        }
    },
    "general": {
        "@stripws": false
    },
    "instance": {
        "@automaticShutdown": "30"
    },
    "javascript": {
        "@use_jsbeautify": true
    },
    "language": {
        "@unusedFunctionArgs": false,
        "tern_defs": {
            "json()": {
                "browser": {
                    "enabled": true
                },
                "ecma5": {
                    "enabled": true
                },
                "jQuery": {
                    "enabled": true
                }
            }
        }
    },
    "python": {
        "@path": "/usr/local/lib/python3.4/dist-packages:/usr/local/lib/python3.5/dist-packages"
    },
    "run": {
        "configs": {
            "@inited": "true",
            "json()": {}
        }
    },
    "welcome": {
        "@first": true
    }
}